<?xml version="1.0" encoding="utf-8"?>
<odoo>

<!--     <template id="successful_ticket_send" name="Success Ticket" page="True"-->
     <template id="successful_construction_ticket_send" name="Success Issues"><!-- odoo11 -->
        <t t-call="website.layout">
            <div class="oe_structure"/>
            <div class="container">
                <div class="row">
                    <div class="col-xs-6 mt16">
                        <div class="alert alert-success">
                            <h5>
                                Your Message/Attachments have been sent successfully.<br/><br/>
                                Thank you,<br/><br/>
                            </h5>
                        </div>
                        <div>
                            <a class="btn btn-primary pull-left mb32" onclick="history.go(-2);" ><span class="hidden-xs">Go Back</span><span class="fa fa-long-arrow-left"/></a>
                       </div>
                    </div>
                    <div class="col-xs-6 mt16">
                        <img width="150" height="150" src="/construction_contracting_issue_tracking/static/description/success.png" alt="show ticket" class="img-square"/>
                   </div>
                </div>
            </div>
            <div class="oe_structure"/>
        </t>
    </template>
    
</odoo>
