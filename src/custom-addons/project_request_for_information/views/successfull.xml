<?xml version="1.0" encoding="utf-8"?>
<odoo>

     <template id="successful_ticket_send" name="Success Ticket"><!-- odoo11 -->
        <t t-call="website.layout">
            <div class="row">
                <div class="container">
                    <div class="col-md-6 mt32">
                        <div class="alert alert-success">
                            <button type="button" class="close" data-dismiss="alert">&amp;times;</button>
                            <h5>
                                Your Message/Attachments have been sent successfully.<br/><br/>
                                Thank you,
                                <br/>
                            </h5>
                            
                        </div>
                    <p>
                        <a class="btn btn-primary pull-left mb32" onclick="history.go(-2);" ><span class="hidden-xs">Go Back</span><span class="fa fa-long-arrow-left"/></a>
                    </p>
                    </div>
                    <div class="col-md-6 mt32">
                        <img width="200" height="200" src="/project_request_for_information/static/description/success.png" alt="show ticket" class="img-square" />
                </div>
            </div>
        </div>
        </t>
    </template>
    
</odoo>
